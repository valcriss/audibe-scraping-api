PORT=3000
NODE_ENV=development

AUDIBLE_BASE_URL=https://www.audible.fr
AUDIBLE_SEARCH_PATH=/search
AUDIBLE_TIMEOUT_MS=10000
AUDIBLE_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/122.0 Safari/537.36
AUDIBLE_ACCEPT_LANGUAGE=fr-FR,fr;q=0.9,en-US;q=0.8,en;q=0.7
AUDIBLE_DETAILS_URL_MODE=guess

API_RATE_LIMIT_ENABLED=false
API_RATE_LIMIT_WINDOW_MS=60000
API_RATE_LIMIT_MAX=60

OUTBOUND_CONCURRENCY=2
OUTBOUND_MIN_TIME_MS=500

REDIS_ENABLED=false
REDIS_URL=redis://localhost:6379
SEARCH_CACHE_TTL_SECONDS=86400

DB_ENABLED=false
DB_HOST=localhost
DB_USER=audibe
DB_PASSWORD=audibe
DB_NAME=audibe
DATABASE_URL=postgresql://audibe:audibe@localhost:5432/audibe
